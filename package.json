{
  "name": "ember-cli-eslint",
  "version": "4.2.3",
  "description": "Ember-cli eslint support, for checking your application matches your coding standards.",
  "keywords": [
    "ember",
    "ember-addon",
    "eslint",
    "eslint ember",
    "eslint ember-cli",
    "lint"
  ],
  "homepage": "https://github.com/ember-cli/ember-cli-eslint",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-eslint/issues"
  },
  "license": "MIT",
  "author": "Jonathan Kingston",
  "files": [
    "blueprints",
    "index.js"
  ],
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-cli/ember-cli-eslint.git"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha node-tests --recursive"
  },
  "dependencies": {
    "broccoli-lint-eslint": "^4.2.1",
    "ember-cli-version-checker": "^2.1.0",
    "rsvp": "^4.6.1",
    "walk-sync": "^0.3.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.0.2",
    "ember-cli": "~2.16.0",
    "ember-cli-babel": "^7.1.2",
    "ember-cli-blueprint-test-helpers": "^0.19.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^2.0.0",
    "ember-cli-qunit": "^4.0.1",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.0",
    "express": "^4.12.3",
    "fs-extra": "^7.0.0",
    "loader.js": "^4.2.3",
    "mocha": "^5.0.0",
    "testdouble": "^3.1.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-eslint"
  }
}
