{
  "name": "ember-cli-eslint",
  "version": "3.0.2",
  "description": "Ember-cli eslint support, for checking your application matches your coding standards.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "blueprints",
    "index.js"
  ],
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha node-tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-cli/ember-cli-eslint.git"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-eslint/issues"
  },
  "homepage": "https://github.com/ember-cli/ember-cli-eslint",
  "author": "Jonathan Kingston",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "chai": "^3.5.0",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-blueprint-test-helpers": "^0.14.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^3.0.0",
    "ember-cli-release": "1.0.0-beta.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "es6-promise": "^4.0.0",
    "express": "^4.12.3",
    "fs-extra": "^0.30.0",
    "loader.js": "^4.0.1",
    "mocha": "^3.0.2",
    "testdouble": "^1.6.0"
  },
  "dependencies": {
    "broccoli-lint-eslint": "^3.1.0",
    "ember-cli-babel": "^5.1.6",
    "js-string-escape": "^1.0.0",
    "rsvp": "^3.2.1",
    "walk-sync": "^0.3.0"
  },
  "keywords": [
    "ember-addon",
    "lint",
    "eslint",
    "eslint ember",
    "eslint ember-cli",
    "ember"
  ],
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-eslint"
  }
}
