{
  "name": "ember-cli-eslint",
  "version": "1.2.1",
  "description": "Ember-cli eslint support, for checking your application matches your coding standards.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanKingston/ember-cli-eslint.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "bugs": {
    "url": "https://github.com/jonathanKingston/ember-cli-eslint/issues"
  },
  "homepage": "https://github.com/jonathanKingston/ember-cli-eslint",
  "author": "Jonathan Kingston",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "~2.1.2",
    "ember-cli": "~1.13.8",
    "ember-cli-app-version": "0.5.0",
    "ember-cli-babel": "~5.1.3",
    "ember-cli-content-security-policy": "^0.4.0",
    "ember-cli-dependency-checker": "~1.0.1",
    "ember-cli-htmlbars": "~0.7.9",
    "ember-cli-htmlbars-inline-precompile": "~0.2.0",
    "ember-cli-ic-ajax": "0.1.4",
    "ember-cli-inject-live-reload": "~1.3.1",
    "ember-cli-qunit": "~1.0.0",
    "ember-cli-sri": "~1.0.3",
    "ember-cli-uglify": "~1.2.0",
    "ember-data": "1.0.0-beta.18",
    "ember-export-application-global": "~1.0.3",
    "ember-try": "0.0.8",
    "eslint-config-ember": "^0.1.1",
    "express": "^4.12.3",
    "phantomjs": "^1.9.16"
  },
  "keywords": [
    "ember-addon",
    "lint",
    "eslint",
    "ember"
  ],
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-eslint"
  },
  "dependencies": {
    "broccoli-lint-eslint": "^1.1.1",
    "js-string-escape": "^1.0.0"
  }
}
